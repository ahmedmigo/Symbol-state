{
    "name" : "Symbol State",
    "identifier" : "symbol.state.plugin",
    "version" : "1.1",
    "description" : "Make states from symbol instance",
    "authorEmail" : "ahmad.migo@gmail.com",
    "URL": "https://github.com/ahmedmigo/Symbol-state",
    "author" : "Ahmed Genaidy",

    "commands" : [
    {
      "script" : "symbolState.js",
      "handler" : "setStatus",
      "shortcut" : "cmd alt a",
      "name" : "Add Symbol State",
      "description" : "Add New State for the selected Symbol",
      "identifier" : "set.State",
      "icon" : "icon-sr.png"
    },
    {
      "script" : "symbolState.js",
      "handler" : "deleteSymbolStates",
      "shortcut" : "cmd alt r",
      "name" : "Delete Symbol State",
      "description" : "Delete existing State from the selected Symbol",
      "identifier" : "delete.State",
      "icon" : "icon-sr.png"
    },
    {
      "script" : "symbolState.js",
      "handler" : "getSymbolStates",
      "shortcut" : "cmd e",
      "name" : "Set Symbol State",
      "description" : "Select State from the Symbol states and apply it to instance",
      "identifier" : "get.State",
      "icon" : "icon-sr.png"
    } 
  ]
}
